{
  "$schema": "https://raw.githubusercontent.com/code-yeongyu/oh-my-opencode/master/assets/oh-my-opencode.schema.json",
  "background_task": {
    "staleTimeoutMs": 300000,
    "defaultConcurrency": 5
  },
  "agents": {
    "sisyphus": {
      "model": "anthropic/claude-sonnet-4-5",
      "enabled": true
    },
    "oracle": {
      "model": "google/gemini-3-pro",
      "variant": "high",
      "enabled": true
    },
    "librarian": {
      "model": "google/gemini-3-flash",
      "enabled": true
    },
    "explore": {
      "model": "anthropic/claude-haiku-4-5",
      "enabled": true
    },
    "multimodal-looker": {
      "model": "google/gemini-3-flash",
      "enabled": true
    },
    "prometheus": {
      "model": "google/gemini-3-pro",
      "variant": "high"
    },
    "metis": {
      "model": "google/gemini-3-pro",
      "variant": "high"
    },
    "momus": {
      "model": "google/gemini-3-pro",
      "variant": "high"
    },
    "atlas": {
      "model": "anthropic/claude-sonnet-4-5"
    }
  },
  "categories": {
    "visual-engineering": {
      "model": "google/gemini-3-pro"
    },
    "ultrabrain": {
      "model": "google/gemini-3-pro",
      "variant": "high"
    },
    "artistry": {
      "model": "google/gemini-3-pro",
      "variant": "high"
    },
    "quick": {
      "model": "google/gemini-3-flash"
    },
    "unspecified-low": {
      "model": "google/gemini-3-flash"
    },
    "unspecified-high": {
      "model": "anthropic/claude-sonnet-4-5"
    },
    "writing": {
      "model": "google/gemini-3-flash"
    }
  }
}
